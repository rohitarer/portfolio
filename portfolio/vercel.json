{
    "version": 2,
    "name": "portfolio",
    "public": true,
  
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  
    "headers": [
      {
        "source": "/(.*)\\.wasm",
        "headers": [{ "key": "Content-Type", "value": "application/wasm" }]
      },
      {
        "source": "/assets/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      },
      {
        "source": "/canvaskit/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      }
    ]
  }
  